<% if @admin == 'admin' %>
	$('.painel-artistas-adm').html('<%= escape_javascript (render partial: "videos/index") %>');
<% else %>
	$('.painel-artistas-art').html('<%= escape_javascript (render partial: "videos/index") %>');
<% end %>


<% if @status == 'success' %>
	<%= render partial: 'mensagem/success'%>
<% elsif @status == 'remover' %>
	<%= render partial: 'mensagem/destroy'%>
<% elsif @status == 'atualizar' %>
	<%= render partial: 'mensagem/update'%>
<% end %>