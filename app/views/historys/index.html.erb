<p id="notice"><%= notice %></p>

<h1>Listing Historys</h1>

<table>
  <thead>
    <tr>
      <th>Artist data</th>
      <th>Historia</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historys.each do |history| %>
      <tr>
        <td><%= history.artist_data %></td>
        <td><%= history.historia %></td>
        <td><%= link_to 'Show', history %></td>
        <td><%= link_to 'Edit', edit_history_path(history) %></td>
        <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History', new_history_path %>
