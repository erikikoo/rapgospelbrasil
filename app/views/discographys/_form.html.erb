<%= simple_form_for(@discography, html: {multipart: true, remote: :true}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.file_field :logo %>
    <%= f.input :nome %>
    <%= f.input :data %>
  </div>

  

  <div class="form-actions">
    <%= f.button :submit, 'Incluir Disco' %>
  </div>
<% end %>
