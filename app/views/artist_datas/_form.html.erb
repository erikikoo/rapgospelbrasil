<%= simple_form_for(@artist_data, html: {multipart: true}) do |f| %>
  <%= f.error_notification %>

  <div class="field">
    <%= f.input :nome %><br>    
  </div>

  <%= f.input :logo, as: :file %>

  <div class="field">
    <%= f.input :historia %><br>    
  </div>
  <fieldset>
      <legend>Contato</legend>
      <%= f.simple_fields_for :emails do |e|%>             
         <%= e.input :email %><br>
      <% end %>

      <%= f.simple_fields_for :phones do |p|%>  
          <%= p.input :telefone %>
      <% end %>

  </fieldset>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
